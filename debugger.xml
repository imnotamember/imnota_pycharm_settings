<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="autoExpressions" value="false" />
      <sort-values>true</sort-values>
      <option name="valueLookupDelay" value="100" />
    </data-views>
    <general>
      <scroll-to-center>true</scroll-to-center>
    </general>
    <debuggers>
      <debugger id="python">
        <configuration>
          <option name="valuesPolicy" value="ON_DEMAND" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>